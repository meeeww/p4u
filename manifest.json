{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Pick4U",
        "version": "1.0.0",
        "minimum-overwolf-version": "0.199.0.15",
        "author": "Developer Name",
        "icon": "IconMouseOver.png",
        "icon_gray": "IconMouseNormal.png",
        "description": "Demo App"
    },
    "data": {
        "start_window": "MainWindow",
        "windows": {
            "MainWindow": {
                "file": "index.html",
                "transparent": false,
                "resizable": true,
                "use_os_windowing": true,
                "size": {
                    "width": 700,
                    "height": 400
                },
                "min_size": {
                    "width": 400,
                    "height": 400
                }
            }
        }
    }
}